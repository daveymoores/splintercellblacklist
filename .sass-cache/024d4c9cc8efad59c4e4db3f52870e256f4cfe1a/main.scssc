html, .age_gate {
	height: 120%;
	
	img.btb_logo {
		position: absolute;
		left: 50%;
		display: block;
		margin-left: -200px;
		margin-top: 140px;
		width: 400px;
	}

	footer {
		margin-top: -30px;
	}
	
	#sc_form-container {
		
		height: 800px;
		
		form {
			width: 35%;
			top: 350px;
			position: absolute;
			left: 50%;
			margin-left: -17.5%;
			padding: 2%;
			background: black url(../assets/images/about_bg.jpg) top center no-repeat;
			-webkit-border-radius: 5px;
			-moz-border-radius: 5px;
			border-radius: 5px;
			&.shadow {
			   -moz-box-shadow:    0 0 5px #4FA199;
			   -webkit-box-shadow: 0 0 5px #4FA199;
			   box-shadow:         0 0 5px #4FA199;
			}
			
			h3 {
				text-align: center;
				font-size: 18px;
				font-family: 'quantico_bold', helvetica, sans-serif;
				color: white;
			}
			
			input[type=submit], input[type=text] {
				text-align: center;
				display: block;
				margin: auto;
				-webkit-border-radius: 2px;
				-moz-border-radius: 2px;
				border-radius: 2px;
				font-family: 'quantico_reg', helvetica, sans-serif;
			}
			
			input[type=submit] {
				margin: 30px auto;
				text-transform: uppercase;
				background: none;
				border: none;
				font-family: 'quantico_bold', helvetica, sans-serif;
				color: black;
				background: black url(../assets/images/nav_bg-blue.png) repeat-x top left;
				-webkit-border-radius: 2px;
				-moz-border-radius: 2px;
				border-radius: 2px;
				padding: 5px 16px;
			}
			
			label {
				color: white;
				font-family: 'quantico_bold', helvetica, sans-serif;
				text-transform: uppercase;
				text-align: center;
				margin-top: 30px;
			}
			
		}
		
	}	
}

#preloader {
    position:absolute;
    top:0;
    left:0;
    right:0;
    bottom:0;
    background-color:#000; /* change if the mask should have another color then white */
    z-index:99999; /* makes sure it stays on top */
    opacity: 0.8;
}

#status {
    width:200px;
    height:200px;
    position:absolute;
    left:50%; /* centers the loading animation horizontally one the screen */
    top:10%; /* centers the loading animation vertically one the screen */
    background-image:url(../assets/images/382.gif); /* path to your loading animation */
    background-repeat:no-repeat;
    background-position:center;
    margin:-100px 0 0 -100px; /* is width and height divided by two */
}

