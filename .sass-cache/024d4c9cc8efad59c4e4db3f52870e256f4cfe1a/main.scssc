:@template"¨./**main body styles**/

body {
	background: black url(../assets/images/background_blue-fade.jpg) no-repeat fixed top center;
}

article {
	background: transparent url(../assets/images/grid_pattern.png) top center repeat;
	position: relative;
	width: 1580px;
	overflow: hidden;
	margin: 0px auto;
	display: block;
}

section.content {
	height: 650px;
	margin: 85px auto;
	width: 100%;

	h2 {
		font-family: 'tango', Helvetica, Sans-serif;
		color: white;
		font-size: 26px;
		font-weight: 300;
		margin: 23px;

		&.black {
			color: black;
		}
	} 

	&.weapons {
		background-color: white;
		-moz-box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);
		-webkit-box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);
		box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);

		background: white url(../assets/images/weapons_bg.jpg) 50% 0 repeat fixed;
		
		.row {
			height: 100%;
			
			.columns {
				height: 100%
			}

			h3 {
				font-family: 'quantico_BOLD', Helvetica, sans-serif; 
				font-size: 18px;
				margin: 0px 0px 0px 22px;
			}
		}
	}

	&.screenshots {
		background-color: black;
		-moz-box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);
		-webkit-box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);
		box-shadow: 0px 0px 10px rgba(0,0,0, 0.5);

		background: black url(../assets/images/screenshot_bg.jpg) 50% 0 repeat fixed;
	}

	&.flash_wrapper {
		margin: 0px auto 85px;
		height: 690px;

		.btb_game {
			height: 690px;
			background: url(../assets/images/flash_frame.png) no-repeat center;
			overflow: hidden;
			position: relative;

			.btb_holder {
				height: 650px;
				width: 950px;
				background-color: black;
				-webkit-border-radius: 7px;
				-moz-border-radius: 7px;
				border-radius: 7px;

				margin: 19px auto;
				position: absolute;
				left: 50%;
				margin-left: -475px;
				overflow: hidden;
				scroll: none;
			}
		}
	}
	
	object {
		overflow: hidden;
		position: relative;
		-webkit-border-radius: 7px;
		-moz-border-radius: 7px;
		border-radius: 7px;
	}

	&.instructional {
		height: 400px;
		margin: 100px auto 0px;
		.holder_instructional {
				background: url(../assets/images/instructional_wrapper.png) no-repeat center;
				height: 180px;
				padding: 20px 0px 0px 35px;

				h3 {
					font-size: 18px;
					color: white;
					font-family: 'tango', helvetica, sans-serif;
				}

				p {
					color: white;
					font-weight: normal;
					font-size: 13px;
					width: 61%;
					font-family: 'quantico_reg', Helvetica, sans-serif; 
				}
		}

		.column, .columns {
			padding: 0px;
		}

		img {
			margin: 35px 0px 20px 18px;

			&.sam {
				position: absolute;
